{
    "data": {
        "test_idl": "./data/crowd2/crowd_test.idl",
        "train_idl": "./data/crowd2/crowd_train.idl",
        "idl_mean": "./data/brainwash_mean.npy"
    },
    "solver": {
        "base_lr": 0.002,
        "max_iter": 800001,
        "test_iter": 599,
        "test_interval": 4000,
        "momentum": 0.9,
        "random_seed": 2,
        "clip_gradients": 0.1,
        "gamma": 0.8,
        "stepsize": 10000,
        "weights": "./data/reinspect_800000.h5"
    },
    "logging": {
        "display_interval": 100,
        "snapshot_prefix": "./data/snapshot/crowd_detection",
        "snapshot_interval": 4000,
        "schematic_path": "/tmp/reinspect_crowd_detection.png",
        "graph_prefix": "/tmp/history",
        "log_file": "/tmp/log_crowd.txt",
        "graph_interval": 500
    },
    "net": {
        "init_range": 0.1,
        "max_len": 5,
        "lstm_num_cells": 250,
        "dropout_ratio": 0.15,
        "hungarian_match_ratio": 0.5,
        "hungarian_permute_matches": true,
        "hungarian_loss_weight": 10,
        "euclidean_loss_weight": 1,
        "googlenet_lr_mult": 4,
        "grid_height": 15,
        "grid_width": 20,
        "img_height": 480,
        "img_width": 640,
        "region_size": 64
    }
}
